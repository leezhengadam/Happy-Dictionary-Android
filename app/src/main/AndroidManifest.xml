<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.qxh151330utdallas.qiyuanhu.quiz_game">
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Happy Dictionary !"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:sharedUserId="android.uid.system"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".About_Us_Activity"></activity>
        <activity android:name="High_Score_Activity"></activity>
        <activity android:name="Level_Choose_Activity"></activity>
        <activity android:name=".Question_Activity_Science"></activity>
        <activity android:name=".Question_Activity_History"></activity>
        <activity android:name=".Question_Activity_Computer"></activity>
        <activity android:name=".Question_Activity_Economic"></activity>


    </application>

</manifest>